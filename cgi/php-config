#! /bin/sh

SED="sed"
prefix="/goinfre/jnoma/homebrew/Cellar/php/8.0.9"
datarootdir="/goinfre/jnoma/homebrew/Cellar/php/8.0.9/share"
exec_prefix="${prefix}"
version="8.0.9"
vernum="80009"
include_dir="${prefix}/include/php"
includes="-I$include_dir -I$include_dir/main -I$include_dir/TSRM -I$include_dir/Zend -I$include_dir/ext -I$include_dir/ext/date/lib"
ldflags=" -L/goinfre/jnoma/homebrew/Cellar/krb5/1.19.2/lib -L/goinfre/jnoma/homebrew/Cellar/openssl@1.1/1.1.1k/lib -L/goinfre/jnoma/homebrew/Cellar/pcre2/10.37_1/lib -L/goinfre/jnoma/homebrew/Cellar/sqlite/3.36.0/lib -L/goinfre/jnoma/homebrew/Cellar/curl/7.78.0/lib -L/goinfre/jnoma/homebrew/Cellar/libffi/3.3_3/lib -L/goinfre/jnoma/homebrew/Cellar/gd/2.3.2/lib -L/goinfre/jnoma/homebrew/opt/gettext/lib -L/goinfre/jnoma/homebrew/opt/gmp/lib -L/goinfre/jnoma/homebrew/Cellar/icu4c/69.1/lib -L/goinfre/jnoma/homebrew/opt/openldap/lib -L/goinfre/jnoma/homebrew/Cellar/oniguruma/6.9.7.1/lib -L/goinfre/jnoma/homebrew/Cellar/unixodbc/2.3.9_1/lib -L/goinfre/jnoma/homebrew/opt/freetds/lib -L/goinfre/jnoma/homebrew/opt/unixodbc/lib -L/goinfre/jnoma/homebrew/opt/libpq/lib -L/goinfre/jnoma/homebrew/opt/aspell/lib -L/goinfre/jnoma/homebrew/Cellar/libsodium/1.0.18_1/lib -L/goinfre/jnoma/homebrew/opt/argon2/lib -L/goinfre/jnoma/homebrew/opt/tidy-html5/lib -L/goinfre/jnoma/homebrew/Cellar/libzip/1.8.0/lib"
libs="  -ltidy -largon2 -lresolv -lncurses -laspell -lpspell -lpq -lpq -lsybdb -lldap -llber -lstdc++ -liconv -lgmp -lintl -lbz2 -lnetwork -lm  -lxml2 -lgssapi_krb5 -lkrb5 -lk5crypto -lcom_err -lssl -lcrypto -lpcre2-8 -lsqlite3 -lz -lcurl -lxml2 -lffi -lssl -lcrypto -lgd -licuio -licui18n -licuuc -licudata -lonig -lodbc -lodbc -lsqlite3 -ledit -lxml2 -lxml2 -lsodium -largon2 -lxml2 -lxml2 -lxml2 -lxslt -lxml2 -lz -lpthread -licucore -lm -lxml2 -lexslt -lxslt -lxml2 -lz -lpthread -licucore -lm -lxml2 -lzip -lz -lssl -lcrypto "
extension_dir='/goinfre/jnoma/homebrew/Cellar/php/8.0.9/pecl/20200930'
man_dir=`eval echo ${datarootdir}/man`
program_prefix=""
program_suffix=""
exe_extension=""
php_cli_binary=NONE
php_cgi_binary=NONE
#Я тут не смогу запустить
configure_options=" '--prefix=/goinfre/jnoma/homebrew/Cellar/php/8.0.9' '--localstatedir=/goinfre/jnoma/homebrew/var' '--sysconfdir=/goinfre/jnoma/homebrew/etc/php/8.0' '--with-config-file-path=/goinfre/jnoma/homebrew/etc/php/8.0' '--with-config-file-scan-dir=/goinfre/jnoma/homebrew/etc/php/8.0/conf.d' '--with-pear=/goinfre/jnoma/homebrew/Cellar/php/8.0.9/share/php/pear' '--enable-bcmath' '--enable-calendar' '--enable-dba' '--enable-exif' '--enable-ftp' '--enable-fpm' '--enable-gd' '--enable-intl' '--enable-mbregex' '--enable-mbstring' '--enable-mysqlnd' '--enable-pcntl' '--enable-phpdbg' '--enable-phpdbg-readline' '--enable-phpdbg-webhelper' '--enable-shmop' '--enable-soap' '--enable-sockets' '--enable-sysvmsg' '--enable-sysvsem' '--enable-sysvshm' '--with-apxs2=/goinfre/jnoma/homebrew/opt/httpd/bin/apxs' '--with-bz2=/Library/Developer/CommandLineTools/SDKs/MacOSX10.14.sdk/usr' '--with-curl' '--with-external-gd' '--with-external-pcre' '--with-ffi' '--with-fpm-user=_www' '--with-fpm-group=_www' '--with-gettext=/goinfre/jnoma/homebrew/opt/gettext' '--with-gmp=/goinfre/jnoma/homebrew/opt/gmp' '--with-iconv=/Library/Developer/CommandLineTools/SDKs/MacOSX10.14.sdk/usr' '--with-kerberos' '--with-layout=GNU' '--with-ldap=/goinfre/jnoma/homebrew/opt/openldap' '--with-libxml' '--with-libedit' '--with-mhash=/Library/Developer/CommandLineTools/SDKs/MacOSX10.14.sdk/usr' '--with-mysql-sock=/tmp/mysql.sock' '--with-mysqli=mysqlnd' '--with-ndbm=/Library/Developer/CommandLineTools/SDKs/MacOSX10.14.sdk/usr' '--with-openssl' '--with-password-argon2=/goinfre/jnoma/homebrew/opt/argon2' '--with-pdo-dblib=/goinfre/jnoma/homebrew/opt/freetds' '--with-pdo-mysql=mysqlnd' '--with-pdo-odbc=unixODBC,/goinfre/jnoma/homebrew/opt/unixodbc' '--with-pdo-pgsql=/goinfre/jnoma/homebrew/opt/libpq' '--with-pdo-sqlite' '--with-pgsql=/goinfre/jnoma/homebrew/opt/libpq' '--with-pic' '--with-pspell=/goinfre/jnoma/homebrew/opt/aspell' '--with-sodium' '--with-sqlite3' '--with-tidy=/goinfre/jnoma/homebrew/opt/tidy-html5' '--with-unixODBC' '--with-xmlrpc' '--with-xsl' '--with-zip' '--with-zlib' '--enable-dtrace' '--with-ldap-sasl' '--with-os-sdkpath=/Library/Developer/CommandLineTools/SDKs/MacOSX10.14.sdk' 'PKG_CONFIG_PATH=/goinfre/jnoma/homebrew/opt/apr/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/openssl@1.1/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/apr-util/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/argon2/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/brotli/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/libidn2/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/libssh2/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/c-ares/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/jemalloc/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/nghttp2/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/openldap/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/rtmpdump/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/zstd/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/curl/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/unixodbc/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/libpng/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/freetype/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/fontconfig/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/jpeg/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/libtiff/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/webp/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/gd/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/libffi/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/pcre/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/readline/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/sqlite/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/xz/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/python@3.9/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/glib/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/gmp/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/icu4c/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/krb5/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/libpq/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/libsodium/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/libzip/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/oniguruma/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/pcre2/lib/pkgconfig:/goinfre/jnoma/homebrew/opt/tidy-html5/lib/pkgconfig' 'PKG_CONFIG_LIBDIR=/usr/lib/pkgconfig:/goinfre/jnoma/homebrew/Library/Homebrew/os/mac/pkgconfig/10.14' 'KERBEROS_CFLAGS= ' 'SASL_CFLAGS=-I/Library/Developer/CommandLineTools/SDKs/MacOSX10.14.sdk/usr/include/sasl' 'SASL_LIBS=-lsasl2'"
php_sapis=" apache2handler cli fpm phpdbg cgi"
ini_dir="/goinfre/jnoma/homebrew/etc/php/8.0/conf.d"
ini_path="/goinfre/jnoma/homebrew/etc/php/8.0"

# Set php_cli_binary and php_cgi_binary if available
for sapi in $php_sapis; do
  case $sapi in
  cli)
    php_cli_binary="${exec_prefix}/bin/${program_prefix}php${program_suffix}${exe_extension}"
    ;;
  cgi)
    php_cgi_binary="${exec_prefix}/bin/${program_prefix}php-cgi${program_suffix}${exe_extension}"
    ;;
  esac
done

# Determine which (if any) php binary is available
if test "$php_cli_binary" != "NONE"; then
  php_binary="$php_cli_binary"
else
  php_binary="$php_cgi_binary"
fi

# Remove quotes
configure_options=`echo $configure_options | $SED -e "s#'##g"`

case "$1" in
--prefix)
  echo $prefix;;
--includes)
  echo $includes;;
--ldflags)
  echo $ldflags;;
--libs)
  echo $libs;;
--extension-dir)
  echo $extension_dir;;
--include-dir)
  echo $include_dir;;
--php-binary)
  echo $php_binary;;
--php-sapis)
  echo $php_sapis;;
--configure-options)
  echo $configure_options;;
--man-dir)
  echo $man_dir;;
--ini-path)
  echo $ini_path;;
--ini-dir)
  echo $ini_dir;;
--version)
  echo $version;;
--vernum)
  echo $vernum;;
*)
  cat << EOF
Usage: $0 [OPTION]
Options:
  --prefix            [$prefix]
  --includes          [$includes]
  --ldflags           [$ldflags]
  --libs              [$libs]
  --extension-dir     [$extension_dir]
  --include-dir       [$include_dir]
  --man-dir           [$man_dir]
  --php-binary        [$php_binary]
  --php-sapis         [$php_sapis]
  --ini-path          [$ini_path]
  --ini-dir           [$ini_dir]
  --configure-options [$configure_options]
  --version           [$version]
  --vernum            [$vernum]
EOF
  exit 1;;
esac

exit 0
